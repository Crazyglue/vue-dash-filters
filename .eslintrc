{
    "plugins": [ "vue" ],
    "rules": {
        "no-process-env": "off",
        "camelcase": [0, { "ignoreDestructuring": true }],
        "vue/max-attributes-per-line": 0,
        "vue/attributes-order": 0,
        "vue/order-in-components": 0,
        "import/prefer-default-export": 0
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "parser": "babel-eslint",
        "sourceType": "module"
    }
}
